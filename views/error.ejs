    <script>
        function sleep(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
 }

        async function animting()
        {
            var ani=document.getElementById("animate");
            ani.style.opacity=0.9;
            await sleep(200);
            ani.style.opacity=0.8;
            await sleep(200);
            ani.style.opacity=0.7;
            await sleep(200);
            ani.style.opacity=0.6;
            await sleep(300);
            ani.style.opacity=0.5;
            await sleep(200);
            ani.style.opacity=0.4;
            await sleep(300);
            ani.style.opacity=0.3;
            await sleep(300);
            ani.style.opacity=0.2;
            await sleep(300);
            ani.style.opacity=0.1;
            await sleep(300);
            ani.style.opacity=0.0;
            await sleep(100);
            ani.style.display="none";

        }

    </script>
